<h1>Listing real_quizzes</h1>

<table>
  <thead>
    <tr>
      <th>Quiz name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_quizzes.each do |real_quiz| %>
      <tr>
        <td><%= real_quiz.quiz_name %></td>
        <td><%= real_quiz.user_id %></td>
        <td><%= link_to 'Show', real_quiz %></td>
        <td><%= link_to 'Edit', edit_real_quiz_path(real_quiz) %></td>
        <td><%= link_to 'Destroy', real_quiz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real quiz', new_real_quiz_path %>
